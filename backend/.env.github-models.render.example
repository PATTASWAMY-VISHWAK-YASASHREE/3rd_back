# Render environment variables for GitHub Models generation
# Add these key/value pairs in Render dashboard.

LLM_PROVIDER=github_models

# Required
GITHUB_MODELS_TOKEN=replace_with_models_read_token

# Recommended low-tier model for Copilot Pro limits
GITHUB_MODELS_MODEL=openai/gpt-4.1-mini

# Optional org attribution
GITHUB_MODELS_ORG=

# API config
GITHUB_MODELS_API_BASE=https://models.github.ai
GITHUB_MODELS_API_VERSION=2022-11-28

# Conservative shared limits
GITHUB_MODELS_RPM_LIMIT=2
GITHUB_MODELS_RPD_LIMIT=150

# Generation tuning
GITHUB_MODELS_MAX_OUTPUT_TOKENS=2048
GITHUB_MODELS_TEMPERATURE=0
